// Change your colors here

@color: #FFFF00;
@background: #000;
@altcolor: #0000FF;

// Color shade definitions. Do not edit

@color-light3: @color + #DDD;
@color-light2: @color + #999;
@color-light1: @color + #666666;
@color-light: @color + #333;
@color-dark: @color - #333;
@color-dark1: @color - #666666;
@color-dark2: @color - #999;
@color-dark3: @color - #DDD;

@background-light3: @background + #DDD;
@background-light2: @background + #999;
@background-light1: @background + #666666;
@background-light: @background + #333;
@background-dark: @background - #333;
@background-dark1: @background - #666666;
@background-dark2: @background - #999;
@background-dark3: @background - #DDD;

@altcolor-light3: @altcolor + #DDD;
